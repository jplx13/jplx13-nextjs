# JPLx13 Environment Variables
# Copy this file to .env.local and update the values as needed

# n8n Webhook URL (optional - currently hardcoded in the application)
# N8N_WEBHOOK_URL=https://jpl13.app.n8n.cloud/webhook/jplx13-form

# Next.js Configuration
NEXT_PUBLIC_APP_NAME=JPLx13
NEXT_PUBLIC_APP_VERSION=1.0.0

# Optional: Analytics (if you want to add analytics later)
# NEXT_PUBLIC_GA_ID=your-google-analytics-id
# NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key

# Optional: Feature flags
# NEXT_PUBLIC_ENABLE_ANALYTICS=false
# NEXT_PUBLIC_ENABLE_DEBUG_MODE=false 